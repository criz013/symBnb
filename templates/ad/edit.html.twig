{% extends 'base.html.twig' %}
{% block title %}Edition d'une annonce{% endblock %}

{% form_theme form "ad/_collection.html.twig" %}

{% block body %}
    <div class="container">
        <h1>Edition d'une annonce</h1>

        {{  form_start(form) }}
        {{  form_widget(form) }}
